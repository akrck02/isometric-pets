config_version=5

[application]

config/name="isopets"
run/main_scene="res://locations/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/image="res://entities/animals/nodes/cat/sprites/cat.png"
boot_splash/use_filter=false
config/icon="res://android_icon.png"

[autoload]

OSManager="*res://shared/managers/os/os.gd"
NfcManager="*res://shared/managers/nfc/nfc.gd"
SignalDatabase="*res://shared/engine/signal_database.gd"
GameSprites="*res://shared/constants/game_sprites.gd"
TimeManager="*res://shared/managers/time/time.gd"
SaveManager="*res://shared/managers/save/save_manager.tscn"
SettingsManager="*res://shared/managers/settings/settings.gd"
UIManager="*res://shared/managers/render/ui/ui.gd"
InputManager="*res://shared/managers/input/input.gd"
Canvas="*res://shared/managers/render/scenes/canvas.gd"
SceneManager="*res://shared/managers/render/scenes/scene.gd"
TilemapManager="*res://shared/managers/render/tilemap.gd"

[display]

window/size/viewport_width=648
window/size/viewport_height=1152
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotNFC/plugin.cfg")

[file_customization]

folder_colors={
"res://bin/": "gray",
"res://dist/": "gray",
"res://entities/": "red",
"res://locations/": "blue",
"res://materials/": "pink",
"res://resources/": "teal",
"res://shaders/": "purple",
"res://shared/": "yellow",
"res://ui/": "orange"
}

